    }
  }
  &.type4 {
    .currency-wrapper > a, .language-wrapper > a,
    .top-links-container ul.links a, .top-links-icon {
      color: $TopLinksBlockFontColor;
    }
    .top-links-container {
      background-color: $TopLinksBlockBackgroundColor;
    }
    .main-nav {
      .main-top-nav {
        background-color: $HeaderMenuBackgroundColor;
      }
    }
  }
  &.type5 {
    .currency-wrapper > a, .language-wrapper > a, .menu-area .links-area .links a {
      color: $TopLinksBlockFontColor;
    }
    .top-links-container {
      background-color: $TopLinksBlockBackgroundColor;
    }
  }
  &.type6 {
    .currency-wrapper > a, .language-wrapper > a,
    .top-links-container ul.links a, .top-links-icon, .top-links-icon a {
      color: $TopLinksBlockFontColor;
