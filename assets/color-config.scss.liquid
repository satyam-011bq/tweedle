{% if settings.custom_header == '1' %}
.header-container {
  &.style1 {
    background-color: $HeaderBackgroundColor;
    .main-nav {
      ul.top-navigation {
        > li {
          &:hover {
            > a{
              color: $HeaderMenuHoverFontColor;
            }
          }
          > a {
            color: $HeaderMenuFontColor;
            &:hover {
              color: $HeaderMenuHoverFontColor;
            }
          }
        }
      }
    }
    .search-area,.menu-icon{
      > a{
        color: $HeaderSearchboxTextFontColor;
      }
    }
    .search-area .form-search {
      background-color: $HeaderSearchboxBackgroundColor;
      border-color: $HeaderSearchboxBorderColor;
      input,.search-button{
        color: $HeaderSearchboxTextFontColor;
      }
      &:before {
        border-bottom-color: $HeaderSearchboxBorderColor;
      }
    }
  }
  &.type1,&.type2,&.style1,&.type4,&.type5,&.type6,&.type7,&.type8,&.type9,&.type10 {
    &.sticky-header {
      .header-wrapper {
        background-color: $StickyHeaderBackgroundColor;
      }
    }
  }
  .header {
    color: $HeaderTextColor;
    a {
      color: $HeaderLinksColor;
      &:hover{
        color: $ThemeColor;
      }
    }
  }
  &.type1 {
    ul.links a, .search-area>a,
    .currency-wrapper > a, .language-wrapper > a,
    .top-links-icon, .top-links-icon a, .menu-icon a {
      color: $TopLinksBlockFontColor;
      &:hover{
        color: $ThemeColor;
      }
    }
    .currency-wrapper,.language-wrapper{
      background-color: #fff;
      a{
        color: #252634;
      }
    }
  }
  &.type2 {
    .top-links-container ul.links a, .top-links-icon, .currency-wrapper > a, .language-wrapper > a {
      color: $TopLinksBlockFontColor;
    }
    .top-links-container {
      background-color: $TopLinksBlockBackgroundColor;
    }
    .main-nav {
      .main-top-nav {
        background-color: $HeaderMenuBackgroundColor;
      }
    }
  }
  &.style1 {
    .currency-wrapper > a, .language-wrapper > a,
